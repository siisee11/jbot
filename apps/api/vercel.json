{
  "version": 2,
  "builds": [
    {
      "src": "/src/jbot/bot/listen.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/jbot/bot/listen.py"
    }
  ],
  "env": {
    "APP_MODULE": "jbot.bot.listen:app"
  }
}
